{
  "name": "@tailwindui/react",
  "version": "0.1.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/tailwindui.esm.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --name tailwindui --tsconfig ./tsconfig.tsdx.json",
    "test": "tsdx test --passWithNoTests",
    "playground": "next dev",
    "lint": "tsdx lint src pages",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "develop",
        "channel": "next"
      }
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.5.0",
    "@testing-library/react": "^10.4.9",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "babel-jest": "^26.3.0",
    "husky": "^4.2.5",
    "next": "9.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-release": "^17.1.1",
    "snapshot-diff": "^0.8.1",
    "tailwindcss": "^1.7.6",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
